@model IEnumerable<YeneMercha.Models.Vote>

@{
    ViewBag.Title = "Result";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}


<table class="table">
    <tr>
        <th>
            Election Title
        </th>
        <th>
            Candidate
        </th>
        <th>
            Vote Number
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item..name)
        </td>
        <td>
            2
        </td>
    </tr>
}

</table>
